{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "barrels": "barrelsby --config .barrelsby.json",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node -r dotenv/config dist/main",
    "del:node_modules": "rimraf node_modules",
    "deps:check": "ncu -t minor",
    "deps:update-run": "ncu -t minor -u",
    "deps:update": "npm-run-all deps:update-run del:node_modules",
    "postdeps:update": "npm install",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "typeorm:cli": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js -d ./src/config/typeorm-migration.config.ts",
    "typeorm:cli-without-config": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js",
    "migration:show": "npm run typeorm:cli -- migration:show",
    "migration:run": "npm run typeorm:cli -- migration:run",
    "migration:create": "npm run typeorm:cli-without-config -- migration:create",
    "migration:generate": "npm run typeorm:cli -- migration:generate",
    "migration:revert": "npm run typeorm:cli -- migration:revert",
    "schema:log": "npm run typeorm:cli -- schema:log",
    "bootstrap": "npm install && docker-compose up",
    "clear_boot": "docker-compose down"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.352.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.4.3",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/typeorm": "^9.0.1",
    "@types/multer": "^1.4.7",
    "barrelsby": "^2.8.0",
    "casdoor-js-sdk": "^0.10.0",
    "casdoor-nodejs-sdk": "^1.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "hbs": "^4.2.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nest-winston": "^1.9.2",
    "nestjs-form-data": "^1.8.7",
    "path": "^0.12.7",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "husky": "8.0.3",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.16",
    "typeorm-naming-strategies": "^4.1.0",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "18.16.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "dotenv": "^16.1.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "lint-staged": "^13.2.2",
    "npm-check-updates": "^16.10.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "overrides": {
    "glob": "7.2.3"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "npx eslint --fix",
      "yarn format",
      "npx prettier --check"
    ]
  }
}
